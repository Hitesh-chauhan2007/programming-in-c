//Write a program to print all factors of a given number.

#include <stdio.h>

int main() {
    printf("Name - Hitesh\nSAP_ID-590027742\nCourse - BCA\nBatch - 6");
    printf("\n-----------------------------------\n");
    int num;

    printf("Enter a positive integer: ");
    scanf("%d", &num);

    printf("Factors of %d are: ", num);
    for (int i = 1; i <= num; i++) {
        if (num % i == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");

    return 0;
}
