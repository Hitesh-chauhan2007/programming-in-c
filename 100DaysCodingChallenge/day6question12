//Write a program to input an integer and check whether it is positive, negative or zero using nested ifâ€“else.
#include<stdio.h>
int main ()
{
	int a;
	printf("enter a number = ");
	scanf("%d",&a);
	if (a>=1)
	{
		printf("its a positive number");
	}
	else if (a==0)
	{
		printf("zero");
	}
	else
	{
		printf("its a negetive number");
	}
	return 0;
}
