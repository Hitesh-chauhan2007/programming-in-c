//Write a program to swap two numbers using a third variable.
#include<stdio.h>
int main()
{
	int a,b,c;
	
	printf("enter the first number = ");
	scanf("%d",&a);
	
	printf("enter the second number = ");
	scanf("%d",&b);
	
	c = a ;
	
	printf("after swapping the numbers will be =\n%d %d",b ,c);
	return 0;
}
