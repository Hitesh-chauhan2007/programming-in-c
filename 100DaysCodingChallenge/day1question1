//Write a program to input two numbers and display their sum

#include<stdio.h>

int main ()

{
	float a,b,c;

	printf("enter your first number = ");
	scanf("%f",&a);
	
	printf("enter your second number = ");
	scanf("%f",&b);
	
	c = a + b ;
	
	printf("the sum of these number will be = %.2f",c);

	return 0;	
}
