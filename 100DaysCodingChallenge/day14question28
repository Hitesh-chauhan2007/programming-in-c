//Write a program to print the product of even numbers from 1 to n.

#include <stdio.h>

int main() {
    printf("Name - Hitesh\nSAP_ID-590027742\nCourse - BCA\nBatch - 6");
    printf("\n-----------------------------------\n");
    int n;
    unsigned long long product = 1;  // Use long long to handle large products

    printf("Enter a positive integer n: ");
    scanf("%d", &n);

    for (int i = 2; i <= n; i += 2) {
        product *= i;
    }

    printf("The product of even numbers from 1 to %d is: %llu\n", n, product);

    return 0;
}
